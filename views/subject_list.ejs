<!doctype html>
<head>
    <meta charset="utf-8" />
    <title>Subject List</title>
</head>
<body>
    <h1>Subject List</h1>
    <div>
        <span><a href="/logtout">Logout</a></span>
    </div>
    <% for (var i = 0; i < subjects.length; i++) { %>

        <% var subject = subjects[i] %>
        <div>
            <span>
                <a href="./question_list?subject=<%=subject.subject %>">
                    <%= subject.desc %>
                </a>
            </span>
        </div>
    <% } %>
    <div>
        <span><a href="/logtout">Logout</a></span>
    </div>
</body>
